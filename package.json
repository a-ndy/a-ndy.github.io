        "test": "cypress run"
    },
    "dependencies": {
        "bootstrap": "^4.5.0",
        "showdown": "^1.9.1"
    },
    "devDependencies": {
        "browserify": "^16.5.1",
        "cypress": "^4.5.0",
        "eslint": "^7.0.0",
        "live-server": "^1.2.1",
        "watchify": "^3.11.1"
    }
}
