    "license": "MIT",
    "scripts": {
        "build": "watchify ./js/main.js --outfile=./js/bundle.js --verbose",
        "build:ci": "browserify ./js/main.js --outfile=./js/bundle.js",
        "sass": "sass --watch styles/custom.scss styles/custom.css",
